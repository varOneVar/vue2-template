{
  "name": "new-vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "npm run serve",
    "qa": "npm run lint && vue-cli-service build --mode qa",
    "build": "vue-cli-service build --modern",
    "prod": "npm run build",
    "lint": "vue-cli-service lint",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "cz": "./node_modules/.bin/cz-customizable",
    "prepare": "husky install",
    "git-push": "winpty sh git.sh",
    "vs-push": "sh git.sh",
    "push": "npm run vs-push"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "current-device": "^0.10.2",
    "dayjs": "^1.10.5",
    "good-storage": "^1.1.1",
    "js-base64": "^3.6.1",
    "normalize.css": "^8.0.1",
    "query-string": "7",
    "uuid": "^8.3.2",
    "vant": "^2.12.27",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-plugin-import": "^1.13.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "commitlint": "^12.1.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^7.0.0",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^11.0.0",
    "mdast-util-from-markdown": "^1.0.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "sass": "^1.35.1",
    "sass-loader": "10.1.1",
    "sass-resources-loader": "^2.2.3",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.2.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "4.46.0",
    "webpack-cli": "^4.7.2"
  },
  "lint-staged": {
    "./src/*.{vue,js,ts}": [
      "vue-cli-service lint",
      "eslint --fix"
    ],
    "./src/*.{css,less,scss,styl,html,htm,vue}": [
      "stylelint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
